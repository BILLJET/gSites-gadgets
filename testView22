	var $j = jQuery.noConflict();
	$j(function() {
		var index = localStorage.getItem("liIndex2");
		$j('li.level1').click(function() {
			localStorage.setItem("liIndex2", $j(this).index());
			var url = window.location.href;
			console.log("Link "+url);
			if(url == "https://sites.google.com/a/contractors.roche.com/testjan/")
			{
				$j("li.level1").addClass('inactive').removeClass("current");
				$j('.main').addClass('current').removeClass('inactive');
			}
			$j("li.level1").addClass('inactive').removeClass("current");
			$j(this).addClass('current').removeClass('inactive');
			//$j('ul#level2').hide();
			//$j('ul#level2', this).show();
		});
		if (index)
			$j("li.level1").eq(index).trigger('click');
	});
	window.onload = j;
</script>
